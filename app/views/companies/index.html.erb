<p id="notice"><%= notice %></p>

<h1>登録企業一覧</h1>

<table>
  <thead>
    <tr>
      <th>企業名</th>
      <th>設立年月日</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @companies.each do |company| %>
      <tr>
        <td><%= company.name %></td>
        <td><%= company.established %></td>
        <td><%= company.url %></td>
        <td><%= link_to 'Show', company %></td>
      </tr>
    <% end %>
  </tbody>
</table>
